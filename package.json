{
  "name": "cookie_audit",
  "description": "Investigate cookie compliance on websites.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt --mode development",
    "dev:firefox": "wxt --mode development -b firefox",
    "dev:debug": "cross-env DEBUG_LEVEL=debug wxt --mode development",
    "build": "wxt build --mode production",
    "build:firefox": "wxt build --mode production -b firefox",
    "build:debug": "wxt build --mode development",
    "zip": "wxt zip --mode production",
    "zip:firefox": "wxt zip --mode production -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@mantine/core": "^7.10.1",
    "@tabler/icons-react": "^3.5.0",
    "@xenova/transformers": "^2.17.2",
    "async-mutex": "^0.5.0",
    "colorjs.io": "^0.5.0",
    "cross-env": "^7.0.3",
    "css-selector-generator": "^3.6.8",
    "fast-average-color": "^9.4.0",
    "pdfmake": "^0.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "simpledotcss": "^2.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "wxt": "^0.18.4"
  }
}
